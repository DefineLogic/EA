<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="game" class="edu.miu.ea.kamal.dayOne.Game">
        <constructor-arg ref="bike"></constructor-arg>
    </bean>

    <bean id="bike" class="edu.miu.ea.kamal.dayOne.Bike">
        <property name="tireSize" value="16"></property>
        <property name="brand" value = "BMX"></property>
    </bean>
    
</beans>